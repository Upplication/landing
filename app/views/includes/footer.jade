//- COSTUMERS SERVICES CONTACT AREA
.customer-service
   .container
      .col.s12
         ul
            li
               a(title='#{help_center.customer_service.text2}') #{help_center.customer_service.text2}
            li.tel.hover-anim
               a(href='#{help_center.customer_service.phone.link}', title='#{help_center.customer_service.phone.alt_tag}')
                  i.icon-phone
                  | !{help_center.customer_service.phone.text_tel}
            li.mail.hover-anim
               a(href='#{help_center.customer_service.email.link}', title='#{help_center.customer_service.email.alt_tag}')
                  i.icon-envelope
                  | #{help_center.customer_service.email.title}
//- FOOTER
footer.page-footer
    //- Footer :: links compilation
    .container.padTop50.padBot50
        .row.hover-anim
            .col.l3.s12.footer-nav-col
                h6 #{external.titles.help}
                ul.footLinks
                    // Temporaly disabled "Help Center" link
                        li
                            a(href='#{external.helpcenter.url}', title='#{external.helpcenter.anchor}') #{external.helpcenter.anchor}
                    li
                        a(href='#{external.tutorials.url}', title='#{external.tutorials.anchor}') #{external.tutorials.anchor}
                    li
                        a(href='#{external.blog.url}', title='#{external.blog.title}', target='_blank',) #{external.blog.anchor}
            .col.l3.s12.footer-nav-col
                h6 #{external.titles.uppl}
                ul.tabs-content-list.footLinks(type='circle')
                    li
                        a(href='#tabs', title='#{external.features.anchor}') #{external.features.anchor}
                    li
                        a(href='#timeline', title='#{external.create_apps.anchor}') #{external.create_apps.anchor}
                    li
                        a(href='#about', title='#{external.apps.anchor}') #{external.apps.anchor}
                    li
                        a(href='#{external.pricing.url}', title='#{external.pricing.anchor}') #{external.pricing.anchor}
            .col.l3.s12.footer-nav-col
                h6 #{external.titles.misc}
                ul.footLinks
                    li
                        a(href='#{external.about.url}', title='#{external.about.anchor}') #{external.about.anchor}
                    li
                        a(href='#{external.company.url}', title='#{external.company.anchor}') #{external.company.anchor}
                    // li
                        a(href='#{external.partners.url}', title='#{external.partners.anchor}') #{external.partners.anchor}
                    li
                        a(href='#{external.breakfasts.url}', target='_blank', title='#{external.breakfasts.anchor}') #{external.breakfasts.anchor}
                    li
                        a.modal-trigger(href='#hiring', title='#{external.hiring}', target='_blank') #{external.hiring}
                    // Temporaly disabled "Press Area" link
                        li
                            a(href='#{external.media_kit.url}', title='#{external.media_kit.anchor}') #{external.media_kit.anchor}
            .col.l3.s12.footer-nav-col
                h6 #{external.titles.legal}
                ul.footLinks
                    li
                        a(href='#{footer.tos.legal.url}', title='#{footer.tos.legal.title}') #{footer.tos.legal.title}
                    li
                        a(href='#{footer.tos.privacy.url}', title='#{footer.tos.privacy.title}') #{footer.tos.privacy.title}
                    li
                        a(href='#{footer.tos.terms_of_use.url}', title='#{footer.tos.terms_of_use.title}') #{footer.tos.terms_of_use.title}
                    li
                        a(href='#{footer.tos.cookies.url}', title='#{footer.tos.cookies.title}') #{footer.tos.cookies.title}
    .clearfix
    //- Footer :: Copyright & Social Media icon buttons
    .footer-copyright
        .container
            span
